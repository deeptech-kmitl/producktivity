{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:qwik/recommended",
    "../../../../.eslintrc.json"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": ["libs/web/shared/ui/tsconfig.*?.json"],
    "ecmaVersion": 2021,
    "sourceType": "module",
    "extraFileExtensions": [".json"],
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["@typescript-eslint"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.mjs", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.mjs", "*.jsx"],
      "rules": {}
    }
  ]
}
