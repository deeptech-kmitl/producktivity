{
  "name": "api-auth",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/api/auth/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "swcrc": "libs/api/shared/config/swc.config.json",
        "outputPath": "dist/apps/api/auth",
        "main": "apps/api/auth/src/main.ts",
        "tsConfig": "apps/api/auth/tsconfig.app.json"
      },
      "configurations": {
        "development": {
          "watch": true
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "api-auth:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "api-auth:build:development"
        },
        "production": {
          "buildTarget": "api-auth:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/api/auth/jest.config.ts"
      }
    }
  },
  "tags": []
}
